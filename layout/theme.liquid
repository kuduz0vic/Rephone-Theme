    {% section 'global-settings' %}
    {% section 'header' %}
    <main
      id="main"
      class="main homepage"
      role="main"
      tabindex="-1"
    >
      {{ content_for_layout }}
    </main>
    {% section 'footer' %}
    {% section 'sidecart' %}

    <script src="{{ 'lazysizes.min.js' | asset_url }}" async="async"></script>
    <script defer src="{{ 'SlickNav/jquery.slicknav.min.js' | asset_url}}" async="async"></script>

    
    <script defer src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>

    <script
      defer src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.10.2/umd/popper.min.js"
      integrity="sha512-nnzkI2u2Dy6HMnzMIkh7CPd1KX445z38XIu4jG1jGw7x5tSL3VBjE44dY4ihMU1ijAQV930SPM12cCFrB18sVw=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    ></script>
    <script
      defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.min.js"
      integrity="sha384-IDwe1+LCz02ROU9k972gdyvl+AESN10+x7tBKgc9I5HFtuNz0wWnPclzo6p9vxnk"
      crossorigin="anonymous"
    ></script>

    <script src="{{ 'main.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'slicknav.js' | asset_url }}" defer="defer"></script>
    <script>
      $(function() {
        $('#slicknav_menu').slicknav();
      });
    </script>

    <script>
      var section = document.getElementById("sidecart");
      if (section) {
        var script1 = document.createElement("script");
        script1.src = "https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js";
        script1.type = "text/javascript";
        section.appendChild(script1);

        var script2 = document.createElement("script");
        script2.innerHTML = "WebFont.load({  google: {    families: ['Ubuntu:300,300italic,400,400italic,500,500italic,700,700italic']  }});"
        section.appendChild(script2);
      }
    </script>
    {{ '//cdn.shopify.com/s/files/1/0194/1736/6592/t/1/assets/booster-page-speed-optimizer.js?23' | script_tag }}

  </body>
  <script>
    function debounce(func, timeout = 300) {
      let timer;
      return(...args) => {
        clearTimeout(timer);
        timer = setTimeout(() => {
          func.apply(this, args);
        }, timeout);
      };
    }
  </script>
  <script type="application/json" data-ajax-cart-initial-state>
    {{ cart | json }}
  </script>

  <script type="module">
    import '{{ "liquid-ajax-cart-v1.10.3.js" | asset_url }}';
  </script>
  <script type="application/json" data-ajax-cart-configuration>
    {
    "addToCartCssClass": "js-my-cart-open"
