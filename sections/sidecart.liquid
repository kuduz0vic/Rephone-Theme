              <div class="sc-payment-icons">
                <img src="{{'Klarna-payment.svg' | asset_url}}" loading="lazy" alt="" class="payment-icon"><img
                  src="{{'Frame.svg' | asset_url}}" loading="lazy" alt="" class="payment-icon"><img src="{{'Frame-1.svg' | asset_url}}"
                  loading="lazy" alt="" class="payment-icon"><img src="{{'Frame-2.svg' | asset_url}}" loading="lazy" alt=""
                  class="payment-icon"><img src="{{'Frame-3.svg' | asset_url}}" loading="lazy" alt="" class="payment-icon"><img
                  src="{{'Frame-4.svg' | asset_url}}" loading="lazy" alt="" class="payment-icon"><img src="{{'Frame-5.svg' | asset_url}}"
